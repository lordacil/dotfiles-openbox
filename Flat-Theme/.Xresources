! X color by github.com/lordacil

*foreground:        #7A7A7A
*background:        #303145
*.foreground:       #d8dee8
*.background:       #2f343f
emacs*foreground:   #6D6168
emacs*background:   #fefefe
URxvt*foreground:   #7A7A7A
XTerm*foreground:   #7A7A7A
UXTerm*foreground:  #7A7A7A
URxvt*background:   [100]#ffffff
XTerm*background:   #fefefe
UXTerm*background:  #303145
URxvt*cursorColor:  #373E4D
XTerm*cursorColor:  #EEEEEE
UXTerm*cursorColor: #EEEEEE  17
URxvt.geometry: 30x25
!URxvt.font: xft::size=8

! clipboard
! update clipboard whenever primary gets updated
URxvt.clipboard.autocopy: true
!URxvt.keysym.M-c: perl:clipboard:copy
!URxvt.keysym.M-v: perl:clipboard:paste
!URxvt.keysym.M-C-v: perl:clipboard:paste_escaped


! black
*.color0  : #4b5262
*.color8  : #434a5a

! red
*.color1  : #bf616a
*.color9  : #b3555e

! green
*.color2  : #a3be8c
*.color10 : #93ae7c

! yellow
*.color3  : #ebcb8b
*.color11 : #dbbb7b

! blue
*.color4  : #81a1c1
*.color12 : #7191b1

! magenta
*.color5  : #b48ead
*.color13 : #a6809f

! cyan
*.color6  : #89d0bA
*.color14 : #7dbba8

! white
*.color7  : #e5e9f0
*.color15 : #d1d5dc

! pink
*.color28 : #f78fb3

! Black color that will not be affected by bold highlighting.
*.color66: #fefefe
*color66:  #fefefe

! Xclock colors.
XClock*foreground: #6D6168
XClock*background: #fefefe
XClock*majorColor:  rgba:6d/61/68/ff
XClock*minorColor:  rgba:6d/61/68/ff
XClock*hourColor:   rgba:6d/61/68/ff
XClock*minuteColor: rgba:6d/61/68/ff
XClock*secondColor: rgba:6d/61/68/ff

! Set depth to make transparency work.
!URxvt*depth: 32


!! URxvt Appearance
URxvt.font: xft:MesloLGS NF:size=9
!URxvt.boldFont: -*-rissole-*
!URxvt.italicFont: -*-rissole-*
!URxvt.boldItalicfont: -*-rissole-*
URxvt.letterSpace: 0
URxvt.lineSpace: 0
!URxvt.geometry: 62x15
URxvt.internalBorder: 24
URxvt.cursorBlink: false
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 24
URxvt.iso14755: false

!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D

!! Copy Paste & Other Extensions
!URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
!URxvt.keysym.M-u: perl:url-select:select_next
URxvt.urlLauncher: firefox
URxvt.underlineURLs: true
URxvt.urlButton: 1


!rofi.modi:                           drun
!rofi.sidebar-mode:                   true
!rofi.terminal:                       urxvt
!rofi.ssh-client:                     ssh
!rofi.ssh-command:                    {terminal} -e "{ssh-client} {host}"
!rofi.opacity:                        100
!rofi.width:                          40
!rofi.lines:                          5
!rofi.columns:                        1
!rofi.font:                           Iosevka 9
!rofi.bw:                             0
!rofi.location:                       0
!rofi.padding:                        30
!rofi.levenshtein-sort:               true
!rofi.case-sensitive:                 false
!rofi.fuzzy:                          false
!rofi.line-margin:                    5
!rofi.separator-style:                solid
!rofi.hide-scrollbar:                 true
!rofi.markup-rows:                    false
!rofi.scrollbar-width: 0
!rofi.color-enabled: true
